{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Metrics": {
    "Enabled": true,
    "InfluxEnabled": true,
    "PrometheusEnabled": true,
    "InfluxUrl": "http://localhost:8086",
    "Database": "AppMetrics",
    "CreateDataBaseIfNotExists": true,
    "Env": "local",
    "Interval": 5
  },
  "AllowedHosts": "*"
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Metrics": {
    "Enabled": true,
    "InfluxEnabled": true,
    "PrometheusEnabled": true,
    "InfluxUrl": "http://localhost:8086",
    "Database": "AppMetrics",
    "CreateDataBaseIfNotExists": true,
    "Env": "local",
    "Interval": 5,
    "UserNameEnvironmentVariable": "INFLUXDB_USERNAME",
    "PasswordEnvironmentVariable": "INFLUXDB_PASSWORD",
    "Tags": {
      "App": "dotnet_app_metrics_prototype_api",
      "Database": "AppMetrics"
    }
  },
  "AllowedHosts": "*"
}
